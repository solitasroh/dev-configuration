{
  "name": "accura-configuration",
  "productName": "accura-configuration",
  "version": "1.0.0",
  "description": "accura configuration",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "solitasroh",
    "email": "78634894+solitasroh@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icons/mac/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "accura_configuration"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            },
            "port": 3001,
            "loggerPort": 9001
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@types/node": "^14.17.32",
    "@types/ping": "^0.4.0",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "electron": "^15.2.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-typescript-enum": "2.0.11",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "prettier": "^2.4.1",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@fontsource/roboto": "4.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@reactivex/rxjs": "6.6.7",
    "@types/react-modal": "3.13.1",
    "@types/react-router": "5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/uuid": "8.3.1",
    "antd": "^4.18.8",
    "babel-plugin-macros": "^3.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "modbus-serial": "^8.0.4",
    "ping": "^0.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.19.5",
    "react-modal": "3.14.4",
    "react-router": "6.0.2",
    "react-router-dom": "^6.0.2",
    "react-select": "^5.2.2",
    "rxjs": "7.4.0",
    "styled-component": "^2.8.0",
    "styled-components": "^5.3.3",
    "styled-reset": "4.3.4",
    "typeface-roboto": "^1.1.13"
  }
}
